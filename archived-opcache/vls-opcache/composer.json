{
    "name": "crossplatformcoder/cpc-opcache",
    "homepage": "http://github.com/CrossPlatformCoder/cpc-opcache",
    "description": "Application for managing PHP's Opcache",
    "type": "application",
    "keywords": [
        "php",
        "service",
        "opcache"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Gareth C Matthews (Crossplatformcoder)",
            "email": "gareth@crossplatformcoder.com",
            "homepage": "http://www.crossplatformcoder.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-stdlib": "^2.7 || ^3.0",
        "zendframework/zend-expressive-zendrouter": "^1.0",
        "zendframework/zend-servicemanager": "^2.7.3 || ^3.0",
        "ocramius/proxy-manager": "^1.0 || ^2.0",
        "zendframework/zend-expressive-zendviewrenderer": "^1.0",
        "crossplatformcoder/cpc-zendexpressive-phpsettings": "^1.0.0",
        "crossplatformcoder/cpc-service-opcache": "^1.0.0",
        "crossplatformcoder/cpc-zendframework-viewhelpers": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^1.1 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "CPC\\Opcache\\": "src/CPCOpcache/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    }
}
